{
  "name": "@mywiseassistant/module-shared-ui",
  "version": "0.1.12",
  "description": "This is a library for MyWiseAssistant Shared UI elements",
  "author": "Lee Wiseman <leew21@hotmail.co.uk>",
  "homepage": "https://mywiseassistant.co.uk",
  "bugs": {
    "url": "https://github.com/MyWiseAssistant/module-shared-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MyWiseAssistant/module-shared-ui"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "postbuild": "cpy . '../dist/' --cwd=src/ --no-overwrite --parents",
    "clean": "rimraf dist",
    "dev": "npm run storybook",
    "lint": "next lint",
    "typescript": "tsc --noEmit",
    "typedoc": "typedoc",
    "test": "jest --maxWorkers=2",
    "test:watch": "npm run test --watch",
    "test:coverage": "jest --coverage --colors --maxWorkers=2",
    "check": "npm run typescript && npm run eslint && npm run prettier",
    "prepare": "husky install",
    "prepublishOnly": "npm run typescript && npm run build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "src/**/*.json": [
      "prettier --write"
    ],
    "src/**/*.scss": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@cloudinary/url-gen": "^1.8.6",
    "classnames": "^2.3.1",
    "cpy-cli": "^4.1.0",
    "css-loader": "^6.7.1",
    "next": "12.2.5",
    "next-transpile-modules": "^9.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "sass": "^1.54.8",
    "style-loader": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@swc/core": "^1.2.226",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "18.7.2",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "babel-loader": "^8.2.5",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "node-sass": "^7.0.1",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^13.0.2",
    "scss-tokenizer": ">=0.4.3",
    "swc-loader": "^0.2.3",
    "ts-jest": "^28.0.7",
    "typescript": "4.7.4",
    "webpack": "^5.74.0"
  }
}
